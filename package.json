{
  "name": "log-md-fixable-links",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "fmt": "prettier --config .prettierrc --write '**/*'",
    "lint": "eslint . --fix --ext .js"
  },
  "dependencies": {
    "got": "^9.6.0",
    "marked": "^4.0.10",
    "mz": "^2.7.0",
    "puppeteer": "^1.20.0"
  },
  "devDependencies": {
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-jest": "^22.17.0",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.5",
    "nock": "^11.3.4",
    "prettier": "^1.18.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.*": [
      "prettier --write",
      "git add"
    ]
  }
}
